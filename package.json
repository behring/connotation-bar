{
  "name": "node-js-getting-started",
  "version": "1.0.0",
  "private": true,
  "description": "A sample Node.js app using Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "development": "cross-env NODE_ENV=dev supervisor -i assets node server",
    "production": "webpack --config webpack.production.config.js -p & cross-env NODE_ENV=production node server",
    "build": "webpack --config webpack.production.config.js"
  },
  "dependencies": {
    "body-parser": "1.12.3",
    "bootstrap-paginator": "^1.0.2",
    "connect-timeout": "^1.7.0",
    "cookie-parser": "^1.3.5",
    "ejs": "2.3.1",
    "express": "4.12.3",
    "leanengine": "^2.0.0",
    "morgan": "^1.8.1",
    "wechat": "^2.1.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "reload": "^1.1.5",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
